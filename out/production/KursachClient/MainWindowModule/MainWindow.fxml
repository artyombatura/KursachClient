<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="MainWindowModule.MainWindowController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <TableView layoutX="14.0" layoutY="32.0" prefHeight="342.0" prefWidth="459.0">
              <columns>
                <TableColumn prefWidth="75.0" text="Фамилия" />
                  <TableColumn prefWidth="75.0" text="Имя" />
                  <TableColumn prefWidth="75.0" text="Отчество" />
                  <TableColumn prefWidth="75.0" text="Оклад" />
                  <TableColumn prefWidth="87.0" text="Отработано" />
                  <TableColumn prefWidth="75.0" text="Норма" />
              </columns>
            </TableView>
            <DatePicker layoutX="477.0" layoutY="146.0" prefHeight="27.0" prefWidth="149.0"
                        fx:id="fromDatePicker" onAction="#fromDatePickerAction"/>
            <Text layoutX="495.0" layoutY="129.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Дата расчета (с/по)" />
            <DatePicker layoutX="477.0" layoutY="187.0" prefHeight="27.0" prefWidth="149.0"
                        fx:id="toDatePicker" onAction="#toDatePickerAction"/>
            <Button layoutX="486.0" layoutY="336.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="131.0" text="Расчитать для выбранного"
                    fx:id="countSelectedButton" onAction="#countSelectedButtonAction">
               <font>
                  <Font size="9.0" />
               </font>
            </Button>
            <Button layoutX="486.0" layoutY="289.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="131.0" text="Расчитать для всех"
                    fx:id="countAllButton" onAction="#countAllButtonAction">
               <font>
                  <Font size="9.0" />
               </font>
            </Button>
            <Button layoutX="515.0" layoutY="32.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="74.0" text="Admin panel"
                    fx:id="adminPanelButton" onAction="#adminPanelButtonAction">
               <font>
                  <Font size="9.0" />
               </font>
            </Button>
         </children>
    </AnchorPane>
  </children>
</VBox>
