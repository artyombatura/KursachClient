<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="AdminLoginModule.AdminLoginController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <TextField fx:id="loginTextField" layoutX="27.0" layoutY="49.0" promptText="Введите login" />
            <TextField fx:id="passwordTextField" layoutX="27.0" layoutY="89.0" promptText="Введиие password" />
            <Button fx:id="loginButton" layoutX="52.0" layoutY="128.0" mnemonicParsing="false" onAction="#loginButtonAction" prefHeight="27.0" prefWidth="95.0" text="Войти" />
         </children>
    </AnchorPane>
  </children>
</VBox>
