<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="AdminCapabilitiesModule.AddEmployeeController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <TextField fx:id="lastNameTextField" layoutX="14.0" layoutY="14.0" promptText="Фамилия" />
            <TextField fx:id="firstNameTextField" layoutX="14.0" layoutY="51.0" promptText="Имя" />
            <TextField fx:id="patronymicTextField" layoutX="14.0" layoutY="92.0" promptText="Отчество" />
            <TextField fx:id="salaryTextField" layoutX="14.0" layoutY="131.0" promptText="Оклад" />
            <Button fx:id="addButton" layoutX="49.0" layoutY="175.0" mnemonicParsing="false" onAction="#addAction" text="Добавить" />
      </children>
    </AnchorPane>
  </children>
</VBox>
