                  <TableColumn fx:id="lastNameColumn" prefWidth="150.0" text="Фамилия" />
                  <TableColumn fx:id="firstNameColumn" minWidth="0.0" prefWidth="154.0" text="Имя" />
                  <TableColumn fx:id="patronymicColumn" minWidth="0.0" prefWidth="170.0" text="Отчество" />
                  <TableColumn fx:id="salaryColumn" prefWidth="145.0" text="Зарплата(на руки)" />
